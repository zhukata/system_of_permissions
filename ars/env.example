# Database Configuration
# Для локальной разработки (приложение запущено локально, база в Docker)
DATABASE_URL=postgresql://ars:ars@localhost:5432/ars

# Для работы внутри Docker сети (приложение тоже в Docker)
# DATABASE_URL=postgresql://ars:ars@postgres:5432/ars

# RabbitMQ Configuration
# Для локальной разработки
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672

# Для работы внутри Docker сети
# RABBITMQ_HOST=rabbitmq
# RABBITMQ_PORT=5672

RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/

# Registry Service URL (для Worker'ов)
REGISTRY_SERVICE_URL=http://localhost:8001

# Application Configuration
APP_NAME=Access Request Service

